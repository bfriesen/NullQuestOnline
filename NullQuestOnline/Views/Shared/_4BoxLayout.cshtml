@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="main-table">
    <tr>
        <td rowspan="2" style="min-width: 240px;">@RenderSection("topleft", required: true)</td>
        <td style="width: 100%; height: 120px;">@RenderSection("topright", required: true)</td>
    </tr>
    <tr>
        <td rowspan="2">@RenderSection("bottomright")</td>
    </tr>
    <tr>
        <td style="height: 200px;">@RenderSection("bottomleft")</td>
    </tr>
</table>